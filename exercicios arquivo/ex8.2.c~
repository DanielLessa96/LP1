#include <stdio.h>
#include <string.h>

int main(){
    char str[25];
    int i = 0;
    FILE* f = fopen("/tmp/guest-Ut3TFE/√Årea de Trabalho/exercicios arquivo/arq8.1.txt", "r");
    while (str != EOF){
          str[i] = fgetc(f);
          printf ("%c\n", str[i]);
          i++;
    }

    fclose(f);
    return 0;
}
          
